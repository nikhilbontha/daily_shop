<%- include('../layout/header') %>
<div class="container">
  <h1>Payments / Orders</h1>
  <table>
    <thead><tr><th>Order ID</th><th>User</th><th>Status</th><th>Amount</th></tr></thead>
    <tbody>
      <% payments.forEach(function(o){ %>
        <tr>
          <td><%= o._id %></td>
          <td><%= o.userId || 'N/A' %></td>
          <td><%= o.status || 'N/A' %></td>
          <td><%= o.total || o.amount || 'N/A' %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  <p><a href="/admin">Back to dashboard</a></p>
</div>
<%- include('../layout/footer') %>
